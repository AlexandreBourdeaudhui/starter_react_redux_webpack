{
  "name": "BaseWebpack",
  "version": "1.0.0",
  "description": "Simple base with Webpack, React & Redux",
  "main": "dist/app.js",
  "author": "AlexandreBourdeaudhui <alexandre.bourdeaudhui@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "test": "test(){ if [ -z $1 ]; then mocha tests/.setup.js 'tests/**/*.test.js'; else mocha tests/.setup.js $1 $2; fi; }; test",
    "lint": "eslint --fix '{app,tests}/**/*.{js,json}'",
    "pretty": "prettier --write '{app,tests}/**/*.{js,json}'",
    "precommit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "4.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.2",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-resolver": "^1.1.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^7.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.10.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandreBourdeaudhui/starter_react_redux_webpack.git"
  },
  "keywords": [
    "Starter",
    "React",
    "Redux",
    "Webpack"
  ],
  "bugs": {
    "url": "https://github.com/AlexandreBourdeaudhui/starter_react_redux_webpack/issues"
  },
  "homepage": "https://github.com/AlexandreBourdeaudhui/starter_react_redux_webpack#readme"
}
